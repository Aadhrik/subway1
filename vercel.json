{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/dashboard-state",
      "dest": "/api/dashboard-state.js"
    },
    {
      "src": "/api/arrivals",
      "dest": "/api/arrivals.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
